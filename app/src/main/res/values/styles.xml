<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--     name="AppTheme"-->
    <style name="Theme.Project_04" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>



    <style name="TimeFrameButton">
        <item name="android:background">@drawable/timeframe_button_selector</item>
        <item name="android:textColor">@color/time_frame_text_selector</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">4dp</item>
    </style>

    <style name="TabStyle">
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingHorizontal="24dp"
        android:paddingVertical="8dp"
        android:textSize="16sp"
    </style>
    <style name="CircleImage" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>